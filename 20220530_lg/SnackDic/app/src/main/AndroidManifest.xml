<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snackdic">

    <queries>
        <package
            android:name="com.sampleapp" />
        <package
            android:name="com.com.nhn.android.nmap" />
        <package
            android:name="net.daum.android.map" />
    </queries>

    <uses-permission
        android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission
        android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chicken"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/chicken"
        android:supportsRtl="true"
        android:theme="@style/Theme.SnackDic">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".listActivity">
        </activity>

        <activity
            android:name=".listActivity2">
        </activity>

        <activity
            android:name=".listActivity3">
        </activity>

        <activity
            android:name=".listEndActivity">
        </activity>

        <activity
            android:name=".random.randomActivity">
        </activity>

        <activity
            android:name=".akinator.akinatorActivity">
        </activity>

        <activity
            android:name=".FBTI.testActivity">
        </activity>

        <activity
            android:name=".random.emptyActivity">
        </activity>

        <activity
            android:name=".random.randompickActivity">
        </activity>

        <activity
            android:name=".FBTI.MBTIRes">
        </activity>

        <activity
            android:name=".FBTI.MBTIRes2">
        </activity>

        <activity
            android:name=".akinator.Aki1Activity">
        </activity>

        <activity android:name=".popupActivity"
            android:theme="@style/popupTheme" android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>